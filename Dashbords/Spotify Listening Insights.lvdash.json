{
  "datasets": [
    {
      "name": "7cab9aea",
      "displayName": "Genre Trends Over Time",
      "queryLines": [
        "SELECT \r\n",
        "    month,\r\n",
        "    genre,\r\n",
        "    total_plays,\r\n",
        "    avg_popularity\r\n",
        "FROM spotify_etl.gold.gold_genre_trends"
      ]
    },
    {
      "name": "32f7b617",
      "displayName": "Top Artists by Listening Time",
      "queryLines": [
        "SELECT\r\n",
        "    artist_name,\r\n",
        "    total_minutes,\r\n",
        "    total_plays,\r\n",
        "    unique_tracks\r\n",
        "FROM spotify_etl.gold.gold_top_artists\r\n",
        "order BY total_minutes DESC\r\n",
        "limit 100"
      ]
    },
    {
      "name": "964249fd",
      "displayName": "Top Tracks by Play Count",
      "queryLines": [
        "SELECT \r\n",
        "    track_name,\r\n",
        "    artist_name,\r\n",
        "    play_count,\r\n",
        "    popularity\r\n",
        "FROM spotify_etl.gold.gold_top_tracks\r\n",
        "ORDER BY play_count DESC\r\n",
        "LIMIT 100"
      ]
    },
    {
      "name": "71b4692a",
      "displayName": "Listening Patterns by Hour and Weekday",
      "queryLines": [
        "SELECT \r\n",
        "  weekday_name,\r\n",
        "  CASE \r\n",
        "    WHEN hour_of_day BETWEEN 6 AND 11 THEN 'Morning'\r\n",
        "    WHEN hour_of_day BETWEEN 12 AND 17 THEN 'Afternoon'\r\n",
        "    WHEN hour_of_day BETWEEN 18 AND 23 THEN 'Evening'\r\n",
        "    ELSE 'Night'\r\n",
        "  END AS time_period,\r\n",
        "  SUM(total_minutes) AS total_minutes\r\n",
        "FROM spotify_etl.gold.gold_listening_patterns\r\n",
        "GROUP BY weekday_name, \r\n",
        "  CASE \r\n",
        "    WHEN hour_of_day BETWEEN 6 AND 11 THEN 'Morning'\r\n",
        "    WHEN hour_of_day BETWEEN 12 AND 17 THEN 'Afternoon'\r\n",
        "    WHEN hour_of_day BETWEEN 18 AND 23 THEN 'Evening'\r\n",
        "    ELSE 'Night'\r\n",
        "  END\r\n",
        "ORDER BY weekday_name, time_period;"
      ]
    },
    {
      "name": "bff42b95",
      "displayName": "User Summary KPI",
      "queryLines": [
        "SELECT \r\n",
        "    total_tracks,\r\n",
        "    unique_artists,\r\n",
        "    total_minutes,\r\n",
        "    favorite_artist,\r\n",
        "    top_track\r\n",
        "FROM spotify_etl.gold.gold_user_listening_summary"
      ]
    }
  ],
